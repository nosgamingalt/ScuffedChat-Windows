{
  "version": 2,
  "functions": {
    "api/index.go": {
      "memory": 1024,
      "maxDuration": 10
    }
  },
  "routes": [
    {
      "src": "/api/(.*)",
      "dest": "/api/index.go"
    },
    {
      "src": "/static/(.*)",
      "dest": "/static/$1"
    },
    {
      "src": "/app",
      "dest": "/static/app.html"
    },
    {
      "src": "/",
      "dest": "/static/index.html"
    }
  ]
}
